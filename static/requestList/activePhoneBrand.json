{
    "pipeline": [
        {
            "$match": {
                "operate": ""
            }
        },
        {
            "$group": {
                "_id": {
                    "brand": "$brand",
                    "deviceType": "$deviceType"
                }
            }
        },
        {
            "$group": {
                "_id": {
                    "brand": "$brand",
                    "deviceType": "$deviceType"
                },
                "num": {
                    "$sum": 1
                }
            }
        },
        {
            "$sort": {
                "num": -1,
                "_id.deviceType": -1
            }
        },
        {
            "$limit": 20
        }
    ],
    "collections":"xxx"
}
