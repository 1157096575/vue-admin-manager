{
    "pipeline": [
        {
            "$match": {
                "operate": "get_token"
            }
        },
        {
            "$group": {
                "_id": {
                    "deviceModel": "$deviceModel",
                    "deviceType": "$deviceType"
                }
            }
        },
        {
            "$group": {
                "_id": {
                    "deviceModel": "$deviceModel",
                    "deviceType": "$deviceType"
                },
                "num": {
                    "$sum": 1
                }
            }
        },
        {
            "$sort": {
                "num": -1,
                "_id.deviceType": -1
            }
        },
        {
            "$limit": 20
        }
    ],
    "collections":"xxx"
}
